    "**/.DS_Store": true,
    "node_modules": true,
    "test-lib": true,
    "lib": true,
    "dist": true,
    "coverage": true,
    "npm": true,
    "coverage.lcov": true,
    "newrelic.js": true,
    "Norma": true,
    "tsconfig.json": true,
    "tslint.json": true,
    "typings.json": true,
    "typings": true,
    ".babelrc": true,
    ".gitignore": true,
    ".env*": true,
    "**/*.yml": true,
    "scripts": true
  }
}