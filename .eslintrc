{
  "extends": "airbnb-base",
  "env": {
    "node": true,
    "es6": true,
    "jest": true
  },
  "globals": {
  },
  "rules": {
    "quotes": [2, "double"],
    "no-underscore-dangle": 0,
    "class-methods-use-this": 0,
