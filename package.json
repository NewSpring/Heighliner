{
  "name": "heighliner",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "pretest": "rm -rf ./dist && mkdir ./dist && npm run build",
    "test": "npm run testonly --",
    "testonly": "mocha --compilers js:babel/register --reporter spec --full-trace --recursive",
    "ci-test": "mocha --compilers js:babel/register --reporter mocha-junit-reporter --full-trace --recursive --reporter-options mochaFile=$CIRCLE_TEST_REPORTS/mocha/junit.xml ./dist",
    "lint": "tslint src/**/*.ts && tslint test/**/*.ts",
    "posttest": "npm run lint && npm run coverage",
    "start": "norma | tsc --watch | supervisor index.js",
    "build": "tsc & norma build",
    "coverage": "istanbul cover ./node_modules/mocha/bin/_mocha -- --reporter dot --compilers js:babel/register --full-trace --recursive",
    "postcoverage": "remap-istanbul --input coverage/coverage.json --type lcovonly --output coverage/lcov.info"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bluebird": "^3.1.1",
    "body-parser": "^1.14.2",
    "casual": "^1.5.3",
    "cors": "^2.7.1",
    "dataloader": "^1.1.0",
    "deasync": "^0.1.6",
    "express": "^4.13.3",
    "graphql": "^0.5.0",
    "graphql-tools": "^0.4.0",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.12.0",
    "mongoose": "^4.3.6",
    "morgan": "^1.7.0",
    "mysql": "^2.10.2",
    "newrelic": "^1.25.0",
    "node-env-file": "^0.1.8",
    "node-mssql": "0.0.1",
    "norma-copy": "^0.4.6",
    "php-unserialize": "0.0.1",
    "redis": "^2.4.2",
    "sequelize": "^3.23.2",
    "tedious": "^1.14.0",
    "typescript": "^1.8.10",
    "typescript-require": "^0.2.9-1"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.9.0",
    "babel-eslint": "^6.0.4",
    "chai": "^3.5.0",
    "eslint": "^2.10.2",
    "graphql-tester": "0.0.4",
    "istanbul": "^0.4.3",
    "mocha": "^2.4.5",
    "mocha-junit-reporter": "^1.11.1",
    "normajs": "^1.7.10",
    "remap-istanbul": "^0.5.1",
    "supervisor": "^0.10.0",
    "tslint": "^3.10.2",
    "typescript": "^1.8.10",
    "typings": "^1.0.4"
  }
}
