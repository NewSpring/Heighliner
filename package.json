{
  "name": "heighliner",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "TEST=true jest",
    "commit": "git-cz",
    "lint": "tslint 'src/**/*.ts' && tslint 'test/**/*.ts'",
    "start": "tsc --watch | supervisor -w lib index.js",
    "build": "tsc && norma build"
  },
  "author": "jbaxleyiii",
