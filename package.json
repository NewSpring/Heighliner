    "testPathDirs": [
      "./src"
    ],
    "testRegex": "./__tests__/.*\\.(ts|tsx|js)$",
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "./src/.*\\.d.ts",
      "./__tests__/.*\\.(ts|tsx|js)$",
      "/node_modules/"
    ],
    "collectCoverage": false
  },
  "license": "ISC",
