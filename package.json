{
  "name": "heighliner",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "pretest": "rm -rf ./lib && mkdir ./lib && npm run build",
    "test": "npm run testonly --",
    "commit": "git-cz",
    "test:watch": "tsc --watch | ava ./lib/test/**/* --watch",
    "copy-schemas": "norma build",
    "testonly": "TEST=true nyc ava ./lib/test/**/*",
    "lint": "tslint 'src/**/*.ts' && tslint 'test/**/*.ts'",
    "posttest": "npm run lint",
    "start": "tsc --watch | supervisor -w lib index.js",
    "build": "tsc && norma build",
    "coverage": "./node_modules/.bin/nyc report --reporter=json",
    "postcoverage": "remap-istanbul --input coverage/coverage-final.json --type lcovonly --output coverage/lcov.info && npm run send:coverage",
    "send:coverage": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "author": "jbaxleyiii",
  "ava": {
    "require": [
      "babel-core/register",
      "babel-polyfill"
    ],
    "babel": "inherit",
    "concurrency": 5,
    "failFast": true
  },
  "license": "ISC",
  "dependencies": {
    "apollo-server": "^0.1.5",
    "apollo-tracer": "^0.1.4",
    "bluebird": "^3.1.1",
    "body-parser": "^1.14.2",
    "casual": "^1.5.3",
    "connect-datadog": "0.0.6",
    "cors": "^2.7.1",
    "datadog-metrics": "^0.3.0",
    "dataloader": "^1.1.0",
    "deasync": "^0.1.6",
    "express": "^4.13.3",
    "geo-from-ip": "^1.0.6",
    "google-geocoding": "^0.1.7",
    "graphql": "^0.6.2",
    "graphql-tools": "^0.6.1",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.12.0",
    "moment": "^2.13.0",
    "mongoose": "^4.5.8",
    "morgan": "^1.7.0",
    "mp3-duration": "^1.0.10",
    "mssql-geoparser": "0.0.1",
    "mysql": "^2.10.2",
    "newrelic": "^1.25.0",
    "node-env-file": "^0.1.8",
    "node-mssql": "0.0.1",
    "norma-copy": "^0.4.6",
    "php-unserialize": "0.0.1",
    "promise-timeout": "^1.0.0",
    "raven": "^0.12.0",
    "redis": "^2.4.2",
    "sequelize": "^3.23.2",
    "strip-tags": "^0.1.1",
    "striptags": "^2.1.1",
    "tedious": "^1.14.0",
    "truncate": "^2.0.0",
    "typescript": "^1.8.10"
  },
  "devDependencies": {
    "ava": "^0.15.1",
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-polyfill": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-runtime": "^6.9.2",
    "commitizen": "^2.8.3",
    "coveralls": "^2.11.9",
    "eslint": "^3.2.2",
    "ghooks": "^1.2.3",
    "graphql-tester": "0.0.4",
    "normajs": "^1.7.10",
    "nyc": "^7.1.0",
    "remap-istanbul": "^0.6.4",
    "supervisor": "^0.11.0",
    "tslint": "^3.10.2",
    "typescript": "^1.8.10",
    "typings": "^1.0.4",
    "word-wrap": "^1.1.0"
  },
  "config": {
    "commitizen": {
      "path": "./scripts/commit"
    },
    "ghooks": {
      "pre-commit": "npm run lint"
    }
  },
  "nyc": {
    "exclude": [
      "**/*.spec.js",
      "**/*.integration.js"
    ]
  }
}
