{
  "name": "heighliner",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "pretest": "rm -rf ./dist && mkdir ./dist && npm run build",
    "test": "npm run testonly --",
    "test:watch": "ava --watch",
    "copy-schemas": "norma build",
    "testonly": "nyc ava",
    "lint": "tslint src/**/*.ts && tslint test/**/*.ts",
    "posttest": "npm run lint",
    "start": "tsc --watch | supervisor -w lib index.js",
    "build": "tsc & norma build",
    "build-coverage": "BABEL_ENV=production babel --out-dir=dist index.js",
    "coverage": "npm run build && npm run build-coverage && ./node_modules/.bin/nyc report --reporter=json",
    "postcoverage": "remap-istanbul --input coverage/coverage-final.json --type lcovonly --output coverage/lcov.info && npm run send:coverage",
    "send:coverage": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "author": "jbaxleyiii",
  "ava": {
    "require": [
      "babel-core/register",
      "babel-polyfill"
    ]
  },
  "license": "ISC",
  "dependencies": {
    "apollo-server": "^0.1.2",
    "bluebird": "^3.1.1",
    "body-parser": "^1.14.2",
    "casual": "^1.5.3",
    "cors": "^2.7.1",
    "dataloader": "^1.1.0",
    "deasync": "^0.1.6",
    "express": "^4.13.3",
    "graphql": "^0.5.0",
    "graphql-tools": "^0.5.0",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.12.0",
    "mongoose": "^4.3.6",
    "morgan": "^1.7.0",
    "mysql": "^2.10.2",
    "newrelic": "^1.25.0",
    "node-env-file": "^0.1.8",
    "node-mssql": "0.0.1",
    "norma-copy": "^0.4.6",
    "php-unserialize": "0.0.1",
    "redis": "^2.4.2",
    "sequelize": "^3.23.2",
    "tedious": "^1.14.0",
    "typescript": "^1.8.10"
  },
  "devDependencies": {
    "ava": "^0.15.1",
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-polyfill": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-runtime": "^6.9.2",
    "codecov": "^1.0.1",
    "coveralls": "^2.11.9",
    "eslint": "^2.10.2",
    "ghooks": "^1.2.3",
    "graphql-tester": "0.0.4",
    "normajs": "^1.7.10",
    "nyc": "^6.4.4",
    "remap-istanbul": "^0.6.4",
    "supervisor": "^0.10.0",
    "tslint": "^3.10.2",
    "typescript": "^1.8.10",
    "typings": "^1.0.4",
    "word-wrap": "^1.1.0"
  },
  "config": {
    "commitizen": {
      "path": "./scripts/commit"
    },
    "ghooks": {
      "pre-commit": "npm run lint"
    }

  }
}
